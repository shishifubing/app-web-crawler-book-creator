{
    "manifest_version": 2,
    "name": "Web-novel scraper",
    "version": "0.0",
    "description": "scrapes web-novels and turns them into ePub books",
    "icons": {
        "48": "icons/black-square48.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.mozilla.org/*"
            ],
            "js": [
                "borderify.js"
            ]
        }
    ]
}